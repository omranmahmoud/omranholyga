# Copy to .env and fill values
MONGODB_URI=mongodb://localhost:27017/yourdb
PORT=5000

# PayPal live or sandbox
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=YOUR_PAYPAL_CLIENT_ID
# Either PAYPAL_SECRET or PAYPAL_CLIENT_SECRET accepted
PAYPAL_SECRET=YOUR_PAYPAL_SECRET

# Optional encryption for stored settings secret
SETTINGS_SECRET_KEY=change-me-32-bytes-key
